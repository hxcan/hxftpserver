<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:tools="http://schemas.android.com/tools" package="com.stupidbeauty.builtinftp.demo" xmlns:android="http://schemas.android.com/apk/res/android">
  <uses-permission android:name="android.permission.INTERNET"/>
  <uses-permission android:name="android.permission.FOREGROUND_SERVICE" />
  
  <application android:icon="@drawable/ic_launcher" android:name="com.stupidbeauty.hxlauncher.application.HxLauncherApplication" android:label="@string/app_name">
    <activity android:name="com.stupidbeauty.builtinftp.demo.LauncherActivity" android:theme="@style/AppTheme" android:label="@string/app_name">
      <intent-filter>
        <action android:name="android.intent.action.MAIN"/>
        <category android:name="android.intent.category.LAUNCHER"/>
      </intent-filter>
    </activity>
        
    <service android:name="com.stupidbeauty.hxlauncher.service.DownloadNotificationService" android:exported="true" tools:ignore="ExportedService">
    </service>
  </application>
</manifest>
